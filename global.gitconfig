[core]
	excludesfile = ~/.gitignore
        editor=/usr/bin/vim
[user]
	email = smihica@gmail.com
	name = smihica
[alias]
        l = log --name-status
        hist = log --pretty=format:\"[32m%h %ad[0m |\t [33m%d[0m %s\" --graph --date=short --decorate
        history = log --oneline --graph --date=short --decorate
        branches = branch -a
        switch  = checkout
        sw      = checkout
        fork    = checkout -b
        join    = merge --no-ff
        stage   = add -p
        staged  = diff --cached
        unstage = reset HEAD
        stashed = stash list
        correct = commit --amend
        histdiff = cherry -v
